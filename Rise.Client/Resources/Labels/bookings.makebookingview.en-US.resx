<?xml version="1.0" encoding="utf-8"?>
<root>
    <xsd:schema id="root" xmlns="" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
        <xsd:element name="root" msdata:IsDataSet="true">
            <xsd:complexType>
                <xsd:choice maxOccurs="unbounded">
                    <xsd:element name="data">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="value" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="comment" type="xsd:string" minOccurs="0"/>
                            </xsd:sequence>
                            <xsd:attribute name="name" type="xsd:string" use="required"/>
                            <xsd:attribute name="xml:space" type="xsd:string" fixed="preserve"/>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:choice>
            </xsd:complexType>
        </xsd:element>
    </xsd:schema>
    <data name="Title" xml:space="preserve">
        <value>Make a booking</value>
    </data>
    <data name="PrefTime" xml:space="preserve">
        <value>Please select the preferred time</value>
    </data>
    <data name="Forenoon" xml:space="preserve">
        <value>10 am</value>
    </data>
    <data name="Noon" xml:space="preserve">
        <value>2 pm</value>
    </data>
    <data name="Afternoon" xml:space="preserve">
        <value>5 pm</value>
    </data>
    <data name="PrefDay" xml:space="preserve">
        <value>Please select the preferred day</value>
    </data>
    <data name="MakeBookingTitle" xml:space="preserve">
        <value>Reserve a timeslot</value>
    </data>
    <data name="MakeBookingMessage" xml:space="preserve">
        <value>Reserve the following timeslot:</value>
    </data>
    <data name="MakeBookingYes" xml:space="preserve">
        <value>Yes</value>
    </data>
    <data name="MakeBookingNo" xml:space="preserve">
        <value>No</value>
    </data>
